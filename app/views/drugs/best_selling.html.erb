<h1>Best Selling Drugs</h1>
<div class="table-responsive">
  <table class="table table-bordered">
    <thead>
      <tr>
        <th>Rank</th>
        <th>Brand Name(s)</th>
        <th>Trade Name</th>
        <th>Generic Name</th>
        <th>Sales 2014</th>
       
      </tr>
    </thead>
    <tbody>
      <% @data.each do |entry| %>
        <tr>
          <td><%= entry[:rank] %></td>
          <td><%= entry[:brand_name] %></td>
          <td><%= entry[:generic_name] %></td>
          <td><%= entry[:sales_q1_2014] %></td>
          <td><%= entry[:change_from_q4_2013] %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
